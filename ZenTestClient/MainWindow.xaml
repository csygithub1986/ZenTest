<Window x:Class="ZenTestClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZenTestClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="600"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="600"/>
            </Grid.ColumnDefinitions>
            <GroupBox Header="函数" />
            <GroupBox Grid.Column="1" Header="参数"/>
            <GroupBox Grid.Column="2" Header="数字结果">
                <Grid/>
            </GroupBox>
            <GroupBox Grid.Column="3" Header="矩阵结果">
                <UniformGrid Name="arrayGrid" Rows="19" Columns="19"/>
                <!--<DataGrid Name="datagrid" HeadersVisibility="None"></DataGrid>-->
            </GroupBox>
        </Grid>
        <GroupBox Header="日志" Grid.Row="1"/>
    </Grid>
</Window>
